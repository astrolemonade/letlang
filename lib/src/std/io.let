module std::io;

effect __println(msg: string) -> @ok;
effect __readline(prompt: string) -> (@ok, string) | (@error, @eof) | (@error, (@io, string));

pub func println(msg: string) -> @ok {
  perform __println(msg);
}

pub func readline(prompt: string) -> (@ok, string) | (@error, @eof) | (@error, (@io, string)) {
  perform __readline(prompt);
}

module std::io;

effect __io_println(msg: string) -> @ok;
effect __io_readline(prompt: string) -> (@ok, string) | (@error, @eof) | (@error, (@io, string));

pub func println(msg: string) -> @ok {
  perform __io_println(msg);
}

pub func readline(prompt: string) -> (@ok, string) | (@error, @eof) | (@error, (@io, string)) {
  perform __io_readline(prompt);
}
